{
  "name": "DS Automator",
  "version": "1.0.0",
  "description": "Плагин предназначен для автоматизации работы дизайнеров, в рамках существующих требований к оформлению дизайн-макетов.",
  "author": "Pavel Neznanov",
  "icon": "./icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavelneznanov/ds-automator-sketch-plugin.git"
  },
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "ds-automator-sketch-plugin",
    "manifest": "src/manifest.json",
    "main": "ds-automator-sketch-plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "dependencies": {}
}
