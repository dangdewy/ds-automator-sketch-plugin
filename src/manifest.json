{
  "name" : "DS Automator",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "./icon.png",
  "description" : "Automation of design system layouts",
  "authorEmail" : "npi@bk.com",
  "author" : "Pavel Neznanov",
  "commands": [
    {
      "script" : "./pages.js",
      "handler" : "pages",
      "shortcut" : "command alt shift p",
      "name" : "Formatting pages",
      "identifier" : "ds-plugin-pages",
      "icon" : "./icons/icon-sr.png",
      "description" : "Sort and format the page list"
    },
    {
      "script" : "./clean-pages.js",
      "handler" : "cleanPages",
      "shortcut" : "command alt shift c",
      "name" : "Clear page numbers",
      "identifier" : "ds-plugin-clean-pages",
      "icon" : "./icons/icon-sr.png",
      "description" : "Removing prefixes in page titles"
    },
    {
      "script" : "./clean-current-artboards.js",
      "handler" : "cleanCurrentArtboards",
      "shortcut" : "command alt shift a",
      "name" : "Clear current artboards",
      "identifier" : "ds-plugin-clean-current-artboards",
      "icon" : "./icons/icon-sr.png",
      "description" : "Removing prefixes in artboards titles"
    }
  ],
  "menu" : {
    "items" : [
        "ds-plugin-pages",
        "ds-plugin-clean-pages",
        "-",
        "ds-plugin-clean-current-artboards"
    ],
    "title" : "DS Automator"
  }
}
